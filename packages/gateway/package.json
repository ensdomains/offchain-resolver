{
  "version": "0.2.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "clean": "rm -fr node_modules dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@ensdomains/offchain-resolver-gateway",
  "author": "Nick Johnson",
  "module": "dist/gateway.esm.js",
  "size-limit": [
    {
      "path": "dist/gateway.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gateway.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.4",
    "@types/chai-as-promised": "^7.1.4",
    "chai-as-promised": "^7.1.1",
    "ganache-cli": "^6.12.2",
    "husky": "^7.0.4",
    "size-limit": "^7.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@chainlink/ccip-read-server": "^0.2.1",
    "@chainlink/ethers-ccip-read-provider": "^0.2.3",
    "@ensdomains/ens-contracts": "^0.0.12",
    "@ensdomains/offchain-resolver-contracts": "^0.2.1",
    "commander": "^8.3.0",
    "dotenv": "^15.0.0",
    "ethers": "^5.6.2"
  }
}
